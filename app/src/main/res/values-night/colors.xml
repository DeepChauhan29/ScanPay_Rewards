<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- This file is intentionally kept empty. -->
    <!-- All colors (including dark theme colors) are now defined in the main colors.xml file -->
    
    <!-- Override light theme colors with dark theme equivalents -->
    <color name="textPrimary">@color/textPrimaryDark</color>
    <color name="textSecondary">@color/textSecondaryDark</color>
    <color name="backgroundPrimaryLight">@color/backgroundPrimaryDark</color>
    <color name="backgroundSecondaryLight">@color/backgroundSecondaryDark</color>
    <color name="cardBackgroundLight">@color/cardBackgroundDark</color>
    <color name="dividerLight">@color/dividerDark</color>
    <color name="borderLight">@color/borderDark</color>
    <color name="bottomNavBackgroundLight">@color/bottomNavBackgroundDark</color>
    <color name="bottomNavUnselectedLight">@color/bottomNavUnselectedDark</color>
    <color name="rippleLight">@color/rippleDark</color>
    <color name="card_unselected">@color/cardUnselectedDark</color>
    
    <!-- Also override the deprecated colors for backwards compatibility -->
    <color name="backgroundPrimary">@color/backgroundPrimaryDark</color>
    <color name="backgroundSecondary">@color/backgroundSecondaryDark</color>
    <color name="backgroundCard">@color/cardBackgroundDark</color>
</resources> 